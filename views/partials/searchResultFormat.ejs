<section class="beer-result container">
        <ul id=beerObject>
            <li class="beer-view" beer_id ='<%= body.data.id %>'>
            <% beers.forEach(beer => { %>
                <h4>Name: <%= body.data.name %></h4>
                <p> Style_name: <%= body.data.style.name %></p>
                <img src= '<% ../../../../public/img/alcohol-alcoholic-beer-1161466.jpg %>''>
                <p> ABV: <%= body.data.abv %> </p>
                <p> IBU Min: <%= body.data.style.ibumin %></p>
                <p> IBU Max: <%= body.data.style.ibumax %></p>
                <p>Description: <%= body.data.style.description %> </p>
           <% }) %> 
              
            </li>
        </ul>
    </section>

